{
  "name": "y",
  "version": "1.0.0",
  "description": "Simple CRUD API using Node.js and Typescript",
  "main": "app.ts",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/app.ts",
    "start:prod": "cross-env NODE_ENV=production node ./bundle/app.js",
    "start:multi": "tsc && NODE_ENV=production node dist/multi.ts",
    "test": "jest --silent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnnaKolesnikova/crud-api.git"
  },
