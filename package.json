  "bugs": {
    "url": "https://github.com/AnnaKolesnikova/crud-api/issues"
  },
  "homepage": "https://github.com/AnnaKolesnikova/crud-api#readme",
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "dotenv": "^16.4.2",
